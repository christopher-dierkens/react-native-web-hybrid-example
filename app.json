{
  "expo": {
    "name": "expo-init-app",
    "slug": "expo-init",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/native/assets/images/icon.png",
    "splash": {
      "image": "./src/native/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["src/native/assets/**/*"],
    "ios": {
      "supportsTablet": true
    }
  }
}
